{
  "name": "vertilia/container",
  "description": "Vertilia ServiceContainer implementation",
  "license": "BSD",
  "type": "library",
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "Vertilia\\Container\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  },
  "require": {
    "php": "^7.1",
    "psr/container": "1.0.0"
  },
  "suggest": {
    "phpunit/phpunit": "To run tests"
  }
}
